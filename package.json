{
  "name": "vapi-self-learning-agents",
  "version": "1.0.0",
  "description": "AI-Powered Call Analytics & Self-Learning Agent Platform for VAPI",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/collection/vapi_all_calls_collector.js",
    "collect": "node scripts/collection/vapi_all_calls_collector.js",
    "sync": "node scripts/upload/airtable_uploader.js upload",
    "sync-full": "npm run collect && npm run sync",
    "dashboard": "start \"\" \"dashboards\\vapi_dashboard.html\"",
    "qci-dashboard": "start \"\" \"dashboards\\qci_analysis_dashboard.html\"",
    "reports": "start \"\" \"reports\\html\"",
    "setup": "npm install",
    "airtable-setup": "node scripts/upload/create_airtable_table.js",
    "airtable-test": "node scripts/upload/test_airtable_upload.js",
    "airtable-upload": "node scripts/upload/airtable_uploader.js upload",
    "airtable-clear": "node scripts/upload/airtable_uploader.js clear",
    "airtable-retry": "node scripts/upload/airtable_uploader.js retry",
    "check-duplicates": "node scripts/analysis/check_airtable_duplicates.js",
    "remove-duplicates": "node scripts/upload/remove_airtable_duplicates.js",
    "verify": "type VAPI_SYNC_VERIFICATION_CHECKLIST.md",
    "analyze": "python scripts/analysis/transcript_analyzer.py",
    "vectorize": "python scripts/training/vector_embedder.py",
    "train": "python scripts/training/agent_trainer.py",
    "test": "echo \"All systems operational - use 'npm run sync-full' for complete data collection and sync\"",
    "test:logging": "DRY_RUN=true node scripts/test_logging_dryrun.js",
    "test:logging:real": "node scripts/test_logging_dryrun.js",
    "logs:view": "cat logs/$(date +%Y-%m-%d).log 2>/dev/null || cat logs/2025-10-18.log",
    "logs:errors": "cat logs/errors/$(date +%Y-%m-%d).log 2>/dev/null || cat logs/errors/2025-10-18.log",
    "logs:watch": "tail -f logs/$(date +%Y-%m-%d).log",
    "api": "node api/server.js",
    "frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run api\" \"npm run frontend\""
  },
  "keywords": [
    "vapi",
    "ai",
    "machine-learning",
    "call-analytics",
    "self-learning",
    "vector-database",
    "conversation-ai"
  ],
  "author": "VAPI Self-Learning Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "airtable": "^0.12.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "openai": "^5.20.1",
    "playwright": "^1.55.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/vapi-self-learning-agents.git"
  },
  "type": "commonjs"
}
